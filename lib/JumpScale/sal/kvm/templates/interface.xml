<interface type='bridge'>
    <mac address='{{macaddress}}'/>
    <source bridge='{{bridge}}'/>
    {{% if qos %}}
    <bandwidth>
        <inbound average='{{rate}}'  burst='{{burst}}'/>
        <outbound average='{{rate}}'  burst='{{burst}}'/>
    </bandwidth>
    {{% end if %}}
    <virtualport type='openvswitch'>
    <parameters profileid='{{name}}'/>
    </virtualport>
d</interface>